#include <stdio.h>

int main() {
    int idade[5];
    int cont;
    int maior, menor;
    for (cont = 0; cont < 5; cont++) {
        printf("Digite a idade da pessoa %d: ", cont + 1);
        scanf("%d", &idade[cont]);
    }
 https://www.onlinegdb.com/#editor_1   maior = idade[0];
    menor = idade[0];
    for (cont = 1; cont < 5; cont++) {
        if (idade[cont] > maior) {
            maior = idade[cont];
        }
        if (idade[cont] < menor) {
            menor = idade[cont];
        }
    }
    printf("Todas as idades digitadas na ordem contrÃ¡ria:\n");
    for (cont = 4; cont >= 0; cont--) {
        printf("%d\n", idade[cont]);
    }
    printf("Maior idade: %d\n", maior);
    printf("Menor idade: %d\n", menor);

    return 0;
}

